<script>
  $(".slider--wrapper").each(function (index) {
    // get sibling track
    let siblingTrack = $(this).siblings(".track--drag");
    // Apply unique IDs to elements
    let wrapperId = "slider--wrapper" + index;
    $(this).attr("id", wrapperId);
    let trackId = "track--drag" + index;
    siblingTrack.attr("id", trackId);
    // create wrapper slider
    let wrapperSlider = new Dragdealer(wrapperId, {
      handleClass: "w-dyn-items",
      loose: true,
      speed: 0.5,
      requestAnimationFrame: true,
      dragStopCallback(x, y) {
        handleSlider.setValue(x, 0, (snap = true));
      },
    });

    // create handle slider
    let handleSlider = new Dragdealer(trackId, {
      handleClass: "handle",
      speed: 0.5,
      requestAnimationFrame: true,
      animationCallback: function (x, y) {
        wrapperSlider.setValue(x, 0, (snap = false));
      },
    });
  });
</script>

<script>
  const tierOptions = ["starter", "growth", "enterprise"];
  let currentPeriod = "year";
  
  for(const tier of tierOptions) {
  	const elem = document.getElementById(`pricing-button-${tier}`);
  	elem.addEventListener("click", () => amplitude.track("Plan Selected On Website", {PlanName: tier}));
	}
  
  const yearMonthToggle = document.getElementById('year-month-toggle');
  
  yearMonthToggle.addEventListener("click", () => {
		currentPeriod = currentPeriod === "year" ? "month" : "year";
    if(currentPeriod === "year") {
    	amplitude.track("Clicks Annual Plan")
    } else {
    	amplitude.track("Clicks Monthly Plan")
    }
  });
</script>
